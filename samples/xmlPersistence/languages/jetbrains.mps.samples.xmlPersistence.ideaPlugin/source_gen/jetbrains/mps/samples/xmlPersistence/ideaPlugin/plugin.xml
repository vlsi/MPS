<!DOCTYPE idea-plugin PUBLIC 'Plugin/DTD' 'http://plugins.intellij.net/plugin.dtd'>
<idea-plugin xmlns:xi="http://www.w3.org/2001/XInclude">
  <id>jetbrains.mps.samples.xmlPersistence</id>
  <name>Sample XML Persistence</name>
  <description>
      Sample persistence of MPS models into xml files</description>
  <version>1.0</version>
  <vendor url="http://www.jetbrains.com/mps/">JetBrains</vendor>
  <idea-version since-build="171.1" until-build="172.1" />
  
  <depends>jetbrains.mps.core</depends>
  
  <extensions defaultExtensionNs="com.intellij">
    <mps.ModelFactoryProvider implementationClass="jetbrains.mps.persistence.XmlModelPersistence" />
    <mps.LanguageLibrary dir="" />
  </extensions>
</idea-plugin>