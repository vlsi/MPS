<!DOCTYPE idea-plugin
  PUBLIC
    "Plugin/DTD"
    "http://plugins.intellij.net/plugin.dtd"
>
<idea-plugin xmlns:xi="http://www.w3.org/2001/XInclude">
  <id>jetbrains.mps.debugger.api.languages</id>
  <name>Debugger Api Languages</name>
  <description>Debugger Api Languages</description>
  <version>1.0</version>
  <vendor url="http://www.jetbrains.com/mps/">JetBrains</vendor>
  <idea-version since-build="3000"></idea-version>
    
  <depends>jetbrains.mps.debugger-api</depends>
    
  <application-components>
    <component>
      <implementation-class>jetbrains.mps.debugger.api.languages.plugin.Plugin_PluginInitializer</implementation-class>
    </component>
  </application-components>
    
  <extensions defaultExtensionNs="com.intellij">
    <mps.LanguageLibrary dir="/"></mps.LanguageLibrary>
    <mps.PluginBundle/>
  </extensions>
</idea-plugin>
